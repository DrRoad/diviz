<?xml version="1.0" encoding="iso-8859-1"?>

<!DOCTYPE program_description PUBLIC "-//ENSTBr//DTD Bioside Program Description 3//EN" "">

<program_description export_date="2009-07-31 19:24:23 +0200">
  <program provider="DecisionDeck" name="alternativesValuesKendall" version="1.0" />
  <from desc_id="S8888" />
  <description>Calculates the correlation index between two rankings of alternatives (represented by lists of ranks).</description>
  <parameters>
    <parameter id="alternativesValuesKendall" type="command" ismandatory="1">
      <name>alternativesValuesKendall</name>
      <description></description>
      <position>100</position>
      <code>genericXMCDAService.py alternativesValuesKendall -S </code>
    </parameter>


    <parameter id="alternatives" ishidden="0" type="input">
      <name>alternatives</name>
      <description>A list of alternatives. Alternatives can be activated or desactivated via the &lt;active&gt; tag.</description>
      <position>102</position>
      <code> alternatives:%s</code>
      <types>
        <type>alternatives</type>
      </types>
      <vdef/>
    </parameter>


    <parameter id="alternativesValues1" ishidden="0" type="input">
      <name>alternativesValues1</name>
      <description>A list of &lt;alternativeValue&gt; containing the rank of each alternative. The &lt;value&gt; should be a numeric value.</description>
      <position>103</position>
      <code> alternativesValues1:%s</code>
      <types>
        <type>alternativesValues</type>
      </types>
      <vdef/>
    </parameter>


    <parameter id="alternativesValues2" ishidden="0" type="input">
      <name>alternativesValues2</name>
      <description>A list of &lt;alternativeValue&gt; containing the rank of each alternative. The &lt;value&gt; should be a numeric value.</description>
      <position>104</position>
      <code> alternativesValues2:%s</code>
      <types>
        <type>alternativesValues</type>
      </types>
      <vdef/>
    </parameter>


    <parameter id="messages-output" ishidden="0" type="output">
      <name>messages</name>
      <description>A list of messages generated by the algorithm.</description>
      <position>205</position>
      <code/>
      <types>
        <type>methodMessages</type>
      </types>
      <vdef>messages.xml</vdef>
    </parameter>




    <parameter id="kendall-output" ishidden="0" type="output">
      <name>kendall</name>
      <description>An &lt;alternativeValue&gt; tag containing the Kendall correlation index of the two rankings of alternatives.</description>
      <position>206</position>
      <code/>
      <types>
        <type>alternativeValue</type>
      </types>
      <vdef>kendall.xml</vdef>
    </parameter>



    <parameter id="TIMEOUT" ishidden="0" type="int">
      <name>Timeout? (0 for no timeout)</name>
      <description>timeout</description>
      <position>101</position>
      <code>-t %s </code>
      <constraint>
        <description>value should be a positive or null integer</description>
        <code>(%d &gt;= 0)</code>
      </constraint>
      <vdef>60</vdef>
    </parameter>
    
    <parameter id="VERBOSE" ishidden="0" type="enum">
      <name>Verbose mode</name>
      <description></description>
      <position>101</position>
      <vlist>
        <item id="T1">
          <description>None</description>
          <code></code>
        </item>
        <item id="T2">
          <description>Moderately verbose</description>
          <code> -v</code>
        </item>
        <item id="T3">
          <description>Very verbose</description>
          <code> -vv</code>
        </item>
        <item id="T4">
          <description>Extremely verbose (include SOAP msgs)</description>
          <code> -vvv</code>
        </item>
      </vlist>
      <vdef>T2</vdef>
    </parameter>

  </parameters>
</program_description>

