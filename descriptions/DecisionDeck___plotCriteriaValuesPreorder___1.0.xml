<?xml version="1.0" encoding="iso-8859-1"?>

<!DOCTYPE program_description PUBLIC "-//ENSTBr//DTD Bioside Program Description 3//EN" "">

<program_description export_date="2009-07-31 19:24:23 +0200">
  <program provider="DecisionDeck" name="plotCriteriaValuesPreorder" version="1.0" />
  <from desc_id="S8888" />
  <description>Generates a graph representing a preorder on the criteria, according to numerical values taken by the criteria.</description>
  <parameters>
    <parameter id="plotCriteriaValuesPreorder" type="command" ismandatory="1">
      <name>plotCriteriaValuesPreorder</name>
      <description></description>
      <position>100</position>
      <code>genericXMCDAService.py plotCriteriaValuesPreorder -S </code>
    </parameter>


    <parameter id="criteria" ishidden="0" type="input">
      <name>criteria</name>
      <description>A list of criteria. Criteria can be activated or desactivated via the &lt;active&gt; tag.</description>
      <position>102</position>
      <code> criteria:%s</code>
      <types>
        <type>criteria</type>
      </types>
      <vdef/>
    </parameter>


    <parameter id="criteriaRanks" ishidden="0" type="input">
      <name>criteriaRanks</name>
      <description>A list of &lt;criterionValue&gt; representing the rank of each criterion in a preorder.</description>
      <position>103</position>
      <code> criteriaRanks:%s</code>
      <types>
        <type>criteriaValues</type>
      </types>
      <vdef/>
    </parameter>


    <parameter id="messages-output" ishidden="0" type="output">
      <name>messages</name>
      <description>A list of messages generated by the algorithm.</description>
      <position>204</position>
      <code/>
      <types>
        <type>methodMessages</type>
      </types>
      <vdef>messages.xml</vdef>
    </parameter>




    <parameter id="criteriaValuesPlot-output" ishidden="0" type="output">
      <name>criteriaValuesPlot</name>
      <description>A string containing the base64 representation of the png image of the graph generated by the R statistical software.</description>
      <position>205</position>
      <code/>
      <types>
        <type>criterionValue</type>
      </types>
      <vdef>criteriaValuesPlot.xml</vdef>
    </parameter>



    <parameter id="TIMEOUT" ishidden="0" type="int">
      <name>Timeout? (0 for no timeout)</name>
      <description>timeout</description>
      <position>101</position>
      <code>-t %s </code>
      <constraint>
        <description>value should be a positive or null integer</description>
        <code>(%d &gt;= 0)</code>
      </constraint>
      <vdef>60</vdef>
    </parameter>
    
    <parameter id="VERBOSE" ishidden="0" type="enum">
      <name>Verbose mode</name>
      <description></description>
      <position>101</position>
      <vlist>
        <item id="T1">
          <description>None</description>
          <code></code>
        </item>
        <item id="T2">
          <description>Moderately verbose</description>
          <code> -v</code>
        </item>
        <item id="T3">
          <description>Very verbose</description>
          <code> -vv</code>
        </item>
        <item id="T4">
          <description>Extremely verbose (include SOAP msgs)</description>
          <code> -vvv</code>
        </item>
      </vlist>
      <vdef>T2</vdef>
    </parameter>

  </parameters>
</program_description>

