<?xml version="1.0" encoding="iso-8859-1"?>



<!DOCTYPE program_description PUBLIC "-//ENSTBr//DTD Bioside Program Description 3//EN" "">

<program_description export_date="2009-09-11 17:56:05 +0200">
  <program provider="DecisionDeck" name="plotCriteriaValuesPreorder" version="1.0" />
  <from desc_id="S8888" />
  <description><![CDATA[Generates a graph representing a preorder on the criteria, according to numerical values taken by the criteria.]]></description>
  <parameters>
    <parameter id="plotCriteriaValuesPreorder-cmdline" type="command" ismandatory="1">
      <name>plotCriteriaValuesPreorder</name>
      <description></description>
      <position>100000</position>
      <code>genericXMCDAService.py plotCriteriaValuesPreorder -S </code>
    </parameter>


    <parameter id="criteria" ishidden="0" type="input">
      <name>criteria</name>
      <description><![CDATA[A list of criteria. Criteria can be activated or desactivated via the <active> tag (true or false). By default (no <active> tag), criteria are considered as active.]]></description>
      <position>100003</position>
      <code> criteria:%s</code>
      <types>
        <type>criteria</type>
      </types>
      <vdef>criteria.xml</vdef>
      <dep/>
    </parameter>



    <parameter id="criteriaRanks" ishidden="0" type="input">
      <name>criteriaValues</name>
      <description><![CDATA[A list of <criterionValue> representing a value taken by each criterion.]]></description>
      <position>100004</position>
      <code> criteriaRanks:%s</code>
      <types>
        <type>criteriaValues</type>
      </types>
      <vdef>criteriaRanks.xml</vdef>
      <dep/>
    </parameter>




    <parameter id="messages" ishidden="0" type="output">
      <name>messages</name>
      <description><![CDATA[A list of messages generated by the algorithm.]]></description>
      <position>2005</position>
      <code/>
      <types>
        <type>methodMessages</type>
      </types>
      <vdef>messages.xml</vdef>
    </parameter>





    <parameter id="criteriaValuesPlot-output" ishidden="0" type="output">
      <name>criteriaValuesPlot</name>
      <description><![CDATA[A string containing the base64 representation of the png image of the graph generated by the R statistical software.]]></description>
      <position>2006</position>
      <code/>
      <types>
        <type>criterionValue</type>
      </types>
      <vdef>criteriaValuesPlot.xml</vdef>
    </parameter>



    <parameter id="TIMEOUT" ishidden="0" type="int">
      <name>Timeout? (0 for no timeout)</name>
      <description>timeout</description>
      <position>100001</position>
      <code>-t %s </code>
      <constraint>
        <description>value should be a positive or null integer</description>
        <code>(%d &gt;= 0)</code>
      </constraint>
      <vdef>60</vdef>
    </parameter>
    
    <parameter id="VERBOSE" ishidden="0" type="enum">
      <name>Verbose mode</name>
      <description></description>
      <position>100002</position>
      <vlist>
        <item id="T1">
          <description>None</description>
          <code></code>
        </item>
        <item id="T2">
          <description>Moderately verbose</description>
          <code> -v</code>
        </item>
        <item id="T3">
          <description>Very verbose</description>
          <code> -vv</code>
        </item>
        <item id="T4">
          <description>Extremely verbose (include SOAP msgs)</description>
          <code> -vvv</code>
        </item>
      </vlist>
      <vdef>T2</vdef>
    </parameter>

  </parameters>
</program_description>

