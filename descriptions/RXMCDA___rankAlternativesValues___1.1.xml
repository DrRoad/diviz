  <program provider="RXMCDA" name="rankAlternativesValues" version="1.1" />
  <from desc_id="S8888" />
  <description><![CDATA[Calculates the rank of alternatives via their overall values. A parameter named maxMin allows to determine if the best value is the highest or the lowest one (by default, the lowest value is ranked first).]]></description>
  <parameters>
    <parameter id="rankAlternativesValues-cmdline" type="command" ismandatory="1">
      <name>rankAlternativesValues</name>
      <description></description>
      <position>100000</position>
      <code>$callXMCDAService rankAlternativesValues-RXMCDA -S </code>
    </parameter>


    <parameter id="maxMin" ishidden="0" type="input">
      <name>maxMin</name>
      <description><![CDATA[The parameter which says if the highest or lowest value is to be ranked first.]]></description>
      <position>100003</position>
      <code/> 
      <types>
        <type>methodParameters</type>
      </types>
      <vdef>maxMin.xml</vdef>
      <dep>(maxMin-dep:value="1") AND (maxMin-use-file-dep:value="1")</dep>

    </parameter>
