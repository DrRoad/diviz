      <name>Use maxMin as input?</name>
      <description>Use maxMin as input?</description>
      <position>3003</position>
      <code/>
      <vdef>1</vdef>
    </parameter>

    <parameter id="maxMin-use-file-dep" ishidden="0" type="boolean">
      <name>Supply maxMin as a file?</name>
      <description>Supply maxMin as a file?</description>
      <position>3003</position>
      <code/>
