<?xml version="1.0" encoding="iso-8859-1"?>


<!DOCTYPE program_description PUBLIC "-//Telecom Bretagne/DTD XML Praxis Program Description 3.0//EN" "http://perso.telecom-bretagne.eu/~bigaret/praxis/dtd/program_description.dtd">

<program_description export_date="2010-11-29 13:57:46 +0000">
  <program provider="RXMCDA" name="computeAlternativesQualification" version="1.0" />
  <from desc_id="S8888" />
  <description><![CDATA[Computes the strength, weakness and qualification of the alternatives. Requires a 0-1 valued outranking relation as input.]]></description>
  <parameters>
    <parameter id="computeAlternativesQualification-cmdline" type="command" ismandatory="1">
      <name>computeAlternativesQualification</name>
      <description></description>
      <position>100000</position>
      <code>$callXMCDAService computeAlternativesQualification-RXMCDA -S </code>
    </parameter>


    <parameter id="alternatives" ishidden="0" type="input">
      <name>alternatives</name>
      <description><![CDATA[A list of alternatives. Alternatives can be activated or desactivated via the <active> tag (true or false). By default (no <active> tag), alternatives are considered as active. ]]></description>
      <position>100003</position>
      <code> alternatives:alternatives.xml</code>
      <types>
        <type>alternatives</type>
      </types>
      <vdef>alternatives.xml</vdef>
      <dep/>
    </parameter>



    <parameter id="alternativesComparisons" ishidden="0" type="input">
      <name>binaryOutrankingRelation</name>
      <description><![CDATA[A 0-1 valued outranking relation. The numeric <value> indicates a the valuation for each <pair> of the relation.]]></description>
      <position>100004</position>
      <code> alternativesComparisons:alternativesComparisons.xml</code>
      <types>
        <type>alternativesComparisons</type>
      </types>
      <vdef>alternativesComparisons.xml</vdef>
      <dep/>
    </parameter>




    <parameter id="messages" ishidden="0" type="output">
      <name>messages</name>
      <description><![CDATA[A list of messages generated by the algorithm.]]></description>
      <position>2005</position>
      <code/>
      <types>
        <type>methodMessages</type>
      </types>
      <vdef>messages.xml</vdef>
    </parameter>





    <parameter id="alternativesWeakness" ishidden="0" type="output">
      <name>alternativesWeakness</name>
      <description><![CDATA[
                    Alternatives' weakness (number of alternatives which "beat" a given alternative).
                ]]></description>
      <position>2006</position>
      <code/>
      <types>
        <type>alternativesValues</type>
      </types>
      <vdef>alternativesWeakness.xml</vdef>
    </parameter>





    <parameter id="alternativesStrength" ishidden="0" type="output">
      <name>alternativesStrength</name>
      <description><![CDATA[
                    Alternatives' strength (number of alternatives which are "beaten" by a given alternative).
                ]]></description>
      <position>2007</position>
      <code/>
      <types>
        <type>alternativesValues</type>
      </types>
      <vdef>alternativesStrength.xml</vdef>
    </parameter>





    <parameter id="alternativesQualification" ishidden="0" type="output">
      <name>alternativesQualification</name>
      <description><![CDATA[
                    Alternatives' qualification (difference between the strength and the weakness).
                ]]></description>
      <position>2008</position>
      <code/>
      <types>
        <type>alternativesValues</type>
      </types>
      <vdef>alternativesQualification.xml</vdef>
    </parameter>



    <parameter id="TIMEOUT" ishidden="0" type="int">
      <name>Timeout? (0 for no timeout)</name>
      <description>timeout</description>
      <position>100001</position>
      <code>-t %s </code>
      <constraint>
        <description>value should be a positive or null integer</description>
        <code>(%d &gt;= 0)</code>
      </constraint>
      <vdef>60</vdef>
    </parameter>
    
    <parameter id="VERBOSE" ishidden="0" type="enum">
      <name>Verbose mode</name>
      <description></description>
      <position>100002</position>
      <vlist>
        <item id="T1">
          <description>None</description>
          <code></code>
        </item>
        <item id="T2">
          <description>Moderately verbose</description>
          <code> -v</code>
        </item>
        <item id="T3">
          <description>Very verbose</description>
          <code> -vv</code>
        </item>
        <item id="T4">
          <description>Extremely verbose (include SOAP msgs)</description>
          <code> -vvv</code>
        </item>
      </vlist>
      <vdef>T2</vdef>
    </parameter>

  </parameters>
</program_description>
