<?xml version="1.0" encoding="iso-8859-1"?>

<!DOCTYPE program_description PUBLIC "-//ENSTBr//DTD Bioside Program Description 3//EN" "">

<program_description export_date="2009-07-31 19:24:25 +0200">
  <program provider="DecisionDeck" name="plotCriteriaComparisons" version="1.0" />
  <from desc_id="S8888" />
  <description>Generates a graph representing a partial preorder on the criteria.</description>
  <parameters>
    <parameter id="plotCriteriaComparisons" type="command" ismandatory="1">
      <name>plotCriteriaComparisons</name>
      <description></description>
      <position>100</position>
      <code>genericXMCDAService.py plotCriteriaComparisons -S </code>
    </parameter>


    <parameter id="cutLevel" ishidden="0" type="input">
      <name>cutLevel</name>
      <description>A real value indicating above which level the valued relation (comparisons) should be considered as validated.</description>
      <position>102</position>
      <code> cutLevel:%s</code>
      <types>
        <type>methodParameters</type>
      </types>
      <vdef/>
      <dep>(cutLevel-dep:value="1")</dep>
    </parameter>
    <parameter id="cutLevel-dep" ishidden="0" type="boolean">
      <name>Use cutLevel as input?</name>
      <description>Use cutLevel as input?</description>
      <position>302</position>
      <code/>
      <vdef>1</vdef>
    </parameter>


    <parameter id="criteria" ishidden="0" type="input">
      <name>criteria</name>
      <description>A list of criteria. Criteria can be activated or desactivated via the &lt;active&gt; tag.</description>
      <position>103</position>
      <code> criteria:%s</code>
      <types>
        <type>criteria</type>
      </types>
      <vdef/>
    </parameter>


    <parameter id="criteriaComparisons" ishidden="0" type="input">
      <name>criteriaComparisons</name>
      <description>A valued relation relative to the partial preorder of the criteria. A numeric &lt;value&gt; indicates a the valuation for each &lt;pair&gt; of the relation.</description>
      <position>104</position>
      <code> criteriaComparisons:%s</code>
      <types>
        <type>criteriaComparisons</type>
      </types>
      <vdef/>
      <dep>(criteriaComparisons-dep:value="1")</dep>
    </parameter>
    <parameter id="criteriaComparisons-dep" ishidden="0" type="boolean">
      <name>Use criteriaComparisons as input?</name>
      <description>Use criteriaComparisons as input?</description>
      <position>304</position>
      <code/>
      <vdef>1</vdef>
    </parameter>


    <parameter id="messages-output" ishidden="0" type="output">
      <name>messages</name>
      <description>A list of messages generated by the algorithm.</description>
      <position>205</position>
      <code/>
      <types>
        <type>methodMessages</type>
      </types>
      <vdef>messages.xml</vdef>
    </parameter>




    <parameter id="criteriaComparisonsPlot" ishidden="0" type="output">
      <name>criteriaComparisonsPlot</name>
      <description>A string containing the base64 representation of the png image of the graph generated by the R statistical software.</description>
      <position>206</position>
      <code/>
      <types>
        <type>criterionValue</type>
      </types>
      <vdef>criteriaComparisonsPlot.xml</vdef>
    </parameter>



    <parameter id="TIMEOUT" ishidden="0" type="int">
      <name>Timeout? (0 for no timeout)</name>
      <description>timeout</description>
      <position>101</position>
      <code>-t %s </code>
      <constraint>
        <description>value should be a positive or null integer</description>
        <code>(%d &gt;= 0)</code>
      </constraint>
      <vdef>60</vdef>
    </parameter>
    
    <parameter id="VERBOSE" ishidden="0" type="enum">
      <name>Verbose mode</name>
      <description></description>
      <position>101</position>
      <vlist>
        <item id="T1">
          <description>None</description>
          <code></code>
        </item>
        <item id="T2">
          <description>Moderately verbose</description>
          <code> -v</code>
        </item>
        <item id="T3">
          <description>Very verbose</description>
          <code> -vv</code>
        </item>
        <item id="T4">
          <description>Extremely verbose (include SOAP msgs)</description>
          <code> -vvv</code>
        </item>
      </vlist>
      <vdef>T2</vdef>
    </parameter>

  </parameters>
</program_description>

