<?xml version="1.0" encoding="iso-8859-1"?>


<!DOCTYPE program_description PUBLIC "-//Telecom Bretagne/DTD XML Praxis Program Description 3.0//EN" "http://perso.telecom-bretagne.eu/~bigaret/praxis/dtd/program_description.dtd">

<program_description export_date="2012-02-09 06:54:37 +0000">
  <program provider="ITTB" name="criteriaDescriptiveStatistics" version="1.0" />
  <from desc_id="S8888" />
  <description><![CDATA[This web service provides statistics about the criteria. It computes for every criterion the average value of criteria's evaluations, the maximum value, the minimum value, the variance, the standard deviation as well as the median value.The criteria's evaluations are supposed to be real or integer numeric values.]]></description>
  <parameters>
    <parameter id="criteriaDescriptiveStatistics-cmdline" type="command" ismandatory="1">
      <name>criteriaDescriptiveStatistics</name>
      <description></description>
      <position>100000</position>
      <code>$callXMCDAService criteriaDescriptiveStatistics-ITTB -S </code>
    </parameter>


    <parameter id="alternatives" ishidden="0" type="input">
      <name>alternatives</name>
      <description><![CDATA[A list of alternatives. Alternatives can be activated or desactivated via the <active> tag (true or false). By default (no <active> tag), alternatives are considered as active. ]]></description>
      <position>100003</position>
      <code> alternatives:alternatives.xml</code>
      <types>
        <type>alternatives</type>
      </types>
      <vdef>alternatives.xml</vdef>
      <dep/>
    </parameter>



    <parameter id="criteria" ishidden="0" type="input">
      <name>criteria</name>
      <description><![CDATA[A list of criteria. Criteria can be activated or desactivated via the <active> tag (true or false). By default (no <active> tag), criteria are considered as active.]]></description>
      <position>100004</position>
      <code> criteria:criteria.xml</code>
      <types>
        <type>criteria</type>
      </types>
      <vdef>criteria.xml</vdef>
      <dep/>
    </parameter>



    <parameter id="performanceTable" ishidden="0" type="input">
      <name>performanceTable</name>
      <description><![CDATA[A performance table. The evaluations should be only real or integer numeric values, i.e. <real> or <integer>.]]></description>
      <position>100005</position>
      <code> performanceTable:performanceTable.xml</code>
      <types>
        <type>performanceTable</type>
      </types>
      <vdef>performanceTable.xml</vdef>
      <dep/>
    </parameter>




    <parameter id="criteriaMeanValues" ishidden="0" type="output">
      <name>criteriaMeanValues</name>
      <description><![CDATA[Average values of criteria's evaluations ]]></description>
      <position>2006</position>
      <code/>
      <types>
        <type>criteriaValues</type>
      </types>
      <vdef>criteriaMeanValues.xml</vdef>
    </parameter>





    <parameter id="criteriaMaximumValues" ishidden="0" type="output">
      <name>criteriaMaximumValues</name>
      <description><![CDATA[Maximum values of criteria's evaluations ]]></description>
      <position>2007</position>
      <code/>
      <types>
        <type>criteriaValues</type>
      </types>
      <vdef>criteriaMaximumValues.xml</vdef>
    </parameter>





    <parameter id="criteriaMinimumValues" ishidden="0" type="output">
      <name>criteriaMinimumValues</name>
      <description><![CDATA[Minimum values of criteria's evaluations ]]></description>
      <position>2008</position>
      <code/>
      <types>
        <type>criteriaValues</type>
      </types>
      <vdef>criteriaMinimumValues.xml</vdef>
    </parameter>





    <parameter id="criteriaVariance" ishidden="0" type="output">
      <name>criteriaVariance</name>
      <description><![CDATA[Variance of criteria's evaluations ]]></description>
      <position>2009</position>
      <code/>
      <types>
        <type>criteriaValues</type>
      </types>
      <vdef>criteriaVariance.xml</vdef>
    </parameter>





    <parameter id="criteriaStandardDeviation" ishidden="0" type="output">
      <name>criteriaStandardDeviation</name>
      <description><![CDATA[Standard deviation of criteria's evaluations ]]></description>
      <position>2010</position>
      <code/>
      <types>
        <type>criteriaValues</type>
      </types>
      <vdef>criteriaStandardDeviation.xml</vdef>
    </parameter>





    <parameter id="criteriaMedianValues" ishidden="0" type="output">
      <name>criteriaMedianValues</name>
      <description><![CDATA[Median values of criteria's evaluations ]]></description>
      <position>2011</position>
      <code/>
      <types>
        <type>criteriaValues</type>
      </types>
      <vdef>criteriaMedianValues.xml</vdef>
    </parameter>





    <parameter id="messages" ishidden="0" type="output">
      <name>messages</name>
      <description><![CDATA[A list of messages generated by the algorithm.]]></description>
      <position>2012</position>
      <code/>
      <types>
        <type>methodMessages</type>
      </types>
      <vdef>messages.xml</vdef>
    </parameter>



    <parameter id="TIMEOUT" ishidden="0" type="int">
      <name>Timeout? (0 for no timeout)</name>
      <description>timeout</description>
      <position>100001</position>
      <code>-t %s </code>
      <constraint>
        <description>value should be a positive or null integer</description>
        <code>(%d &gt;= 0)</code>
      </constraint>
      <vdef>60</vdef>
    </parameter>
    
    <parameter id="VERBOSE" ishidden="0" type="enum">
      <name>Verbose mode</name>
      <description></description>
      <position>100002</position>
      <vlist>
        <item id="T1">
          <description>None</description>
          <code></code>
        </item>
        <item id="T2">
          <description>Moderately verbose</description>
          <code> -v</code>
        </item>
        <item id="T3">
          <description>Very verbose</description>
          <code> -vv</code>
        </item>
        <item id="T4">
          <description>Extremely verbose (include SOAP msgs)</description>
          <code> -vvv</code>
        </item>
      </vlist>
      <vdef>T2</vdef>
    </parameter>

  </parameters>
</program_description>
