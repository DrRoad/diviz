<?xml version="1.0" encoding="iso-8859-1"?>


<!DOCTYPE program_description PUBLIC "-//Telecom Bretagne/DTD XML Praxis Program Description 3.0//EN" "http://perso.telecom-bretagne.eu/~bigaret/praxis/dtd/program_description.dtd">

<program_description export_date="2012-02-23 15:45:41 +0000">
  <program provider="ITTB" name="transitiveReductionCriteriaComparisons" version="1.0" />
  <from desc_id="S8888" />
  <description><![CDATA[This web service applies a transitive reduction on the relation relative to comparisons of the criteria. It generates a file containing the criteriaComparisons tag after processing.]]></description>
  <parameters>
    <parameter id="transitiveReductionCriteriaComparisons-cmdline" type="command" ismandatory="1">
      <name>transitiveReductionCriteriaComparisons</name>
      <description></description>
      <position>100000</position>
      <code>$callXMCDAService transitiveReductionCriteriaComparisons-ITTB -S </code>
    </parameter>


    <parameter id="criteria" ishidden="0" type="input">
      <name>criteria</name>
      <description><![CDATA[A list of criteria. Criteria can be activated or desactivated via the <active> tag (true or false). By default (no <active> tag), criteria are considered as active.]]></description>
      <position>100003</position>
      <code> criteria:criteria.xml</code>
      <types>
        <type>criteria</type>
      </types>
      <vdef>criteria.xml</vdef>
      <dep/>
    </parameter>



    <parameter id="criteriaComparisons" ishidden="0" type="input">
      <name>criteriaComparisons</name>
      <description><![CDATA[A non-valued relation relative to comparisons of the criteria.]]></description>
      <position>100004</position>
      <code> criteriaComparisons:criteriaComparisons.xml</code>
      <types>
        <type>criteriaComparisons</type>
      </types>
      <vdef>criteriaComparisons.xml</vdef>
      <dep/>
    </parameter>




    <parameter id="transitiveReduction" ishidden="0" type="output">
      <name>transitiveReduction</name>
      <description><![CDATA[The transitive reduction is applied and a non-valued relation relative to comparisons of the criteria is generated. ]]></description>
      <position>2005</position>
      <code/>
      <types>
        <type>criteriaComparisons</type>
      </types>
      <vdef>transitiveReduction.xml</vdef>
    </parameter>





    <parameter id="messages" ishidden="0" type="output">
      <name>messages</name>
      <description><![CDATA[A list of messages generated by the algorithm.]]></description>
      <position>2006</position>
      <code/>
      <types>
        <type>methodMessages</type>
      </types>
      <vdef>messages.xml</vdef>
    </parameter>



    <parameter id="TIMEOUT" ishidden="0" type="int">
      <name>Timeout? (0 for no timeout)</name>
      <description>timeout</description>
      <position>100001</position>
      <code>-t %s </code>
      <constraint>
        <description>value should be a positive or null integer</description>
        <code>(%d &gt;= 0)</code>
      </constraint>
      <vdef>60</vdef>
    </parameter>
    
    <parameter id="VERBOSE" ishidden="0" type="enum">
      <name>Verbose mode</name>
      <description></description>
      <position>100002</position>
      <vlist>
        <item id="T1">
          <description>None</description>
          <code></code>
        </item>
        <item id="T2">
          <description>Moderately verbose</description>
          <code> -v</code>
        </item>
        <item id="T3">
          <description>Very verbose</description>
          <code> -vv</code>
        </item>
        <item id="T4">
          <description>Extremely verbose (include SOAP msgs)</description>
          <code> -vvv</code>
        </item>
      </vlist>
      <vdef>T2</vdef>
    </parameter>

  </parameters>
</program_description>
