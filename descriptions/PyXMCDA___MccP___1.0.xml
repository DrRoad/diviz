<?xml version="1.0" encoding="iso-8859-1"?>


<!DOCTYPE program_description PUBLIC "-//Telecom Bretagne/DTD XML Praxis Program Description 3.0//EN" "http://perso.telecom-bretagne.eu/~bigaret/praxis/dtd/program_description.dtd">

<program_description export_date="2011-12-09 08:21:35 +0000" active="0">
  <program provider="PyXMCDA" name="MccP" version="1.0" />
  <from desc_id="S8888" />
  <description><![CDATA[This web service allows to compute ordered clusters of alternatives from an outranking relation with the aim of maximizing the indifference between the alternatives inside the same cluster, and maximizing the preference relations in one direction or another between different clusters.]]></description>
  <parameters>
    <parameter id="MccP-cmdline" type="command" ismandatory="1">
      <name>MccP</name>
      <description></description>
      <position>100000</position>
      <code>$callXMCDAService MccP-PyXMCDA -S </code>
    </parameter>


    <parameter id="alternatives" ishidden="0" type="input">
      <name>alternatives</name>
      <description><![CDATA[A list of alternatives. Alternatives can be activated or dezactivated via the <active> tag (true or false). By default (no <active> tag), alternatives are considered as active. ]]></description>
      <position>100003</position>
      <code> alternatives:alternatives.xml</code>
      <types>
        <type>alternatives</type>
      </types>
      <vdef>alternatives.xml</vdef>
      <dep/>
    </parameter>



    <parameter id="alternativesComparisons" ishidden="0" type="input">
      <name>alternativesComparisons</name>
      <description><![CDATA[The outranking relation used to compare the objects.]]></description>
      <position>100004</position>
      <code> alternativesComparisons:alternativesComparisons.xml</code>
      <types>
        <type>alternativesComparisons</type>
      </types>
      <vdef>alternativesComparisons.xml</vdef>
      <dep/>
    </parameter>



    <parameter id="parameters" ishidden="0" type="input">
      <name>parameters</name>
      <description><![CDATA[The 4 method parameters: min (real value) - the minimum of the valuation domain of the relation; max (real value) - the maximum of the valuation domain of the relation; cut (real value) - the cut level of the relation (for a relation valued between 0(false) and 1(true) we normally have a 0.5 cut level; for a relation valued between -1 and 1 we normally have a 0 cut level); bipolar (boolean value) - the type of cut on the relation (false means a classic, true means bipolar)]]></description>
      <position>100005</position>
      <code> parameters:parameters.xml</code>
      <types>
        <type>methodParameters</type>
      </types>
      <vdef>parameters.xml</vdef>
      <dep/>
    </parameter>




    <parameter id="messages" ishidden="0" type="output">
      <name>messages</name>
      <description><![CDATA[A list of messages generated by the algorithm.]]></description>
      <position>2006</position>
      <code/>
      <types>
        <type>methodMessages</type>
      </types>
      <vdef>messages.xml</vdef>
    </parameter>





    <parameter id="clusters" ishidden="0" type="output">
      <name>clusters</name>
      <description><![CDATA[The clusters names.]]></description>
      <position>2007</position>
      <code/>
      <types>
        <type>categories</type>
      </types>
      <vdef>clusters.xml</vdef>
    </parameter>





    <parameter id="alternativesAffectations" ishidden="0" type="output">
      <name>alternativesAffectations</name>
      <description><![CDATA[The alternatives clusters affectations.]]></description>
      <position>2008</position>
      <code/>
      <types>
        <type>alternativesAffectations</type>
      </types>
      <vdef>alternativesAffectations.xml</vdef>
    </parameter>





    <parameter id="clustersComparisons" ishidden="0" type="output">
      <name>clustersComparisons</name>
      <description><![CDATA[The relation between clusters.]]></description>
      <position>2009</position>
      <code/>
      <types>
        <type>categoriesComparisons</type>
      </types>
      <vdef>clustersComparisons.xml</vdef>
    </parameter>



    <parameter id="TIMEOUT" ishidden="0" type="int">
      <name>Timeout? (0 for no timeout)</name>
      <description>timeout</description>
      <position>100001</position>
      <code>-t %s </code>
      <constraint>
        <description>value should be a positive or null integer</description>
        <code>(%d &gt;= 0)</code>
      </constraint>
      <vdef>60</vdef>
    </parameter>
    
    <parameter id="VERBOSE" ishidden="0" type="enum">
      <name>Verbose mode</name>
      <description></description>
      <position>100002</position>
      <vlist>
        <item id="T1">
          <description>None</description>
          <code></code>
        </item>
        <item id="T2">
          <description>Moderately verbose</description>
          <code> -v</code>
        </item>
        <item id="T3">
          <description>Very verbose</description>
          <code> -vv</code>
        </item>
        <item id="T4">
          <description>Extremely verbose (include SOAP msgs)</description>
          <code> -vvv</code>
        </item>
      </vlist>
      <vdef>T2</vdef>
    </parameter>

  </parameters>
</program_description>
